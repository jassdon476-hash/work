const inputs = document.querySelectorAll('input[name^="otpCode-"]');
const sendBtn = document.getElementById('smsend');
const errorDiv = document.getElementById('errsms');

// Helper to update empty class
function updateInputClasses() {
    inputs.forEach(input => {
        if (input.value === '' || input.value === '_') {
            input.classList.add('enzoog8vk');
        } else {
            input.classList.remove('enzoog8vk');
        }
    });
}

// Helper to check if all 6 digits are entered
function isCodeComplete() {
    return Array.from(inputs).every(input => /^\d$/.test(input.value));
}

// Helper to hide error if code is complete
function updateErrorVisibility() {
    if (isCodeComplete()) {
        errorDiv.style.display = 'none';
    }
}

// Initialize inputs with '_'
inputs.forEach(input => {
    input.value = '_';
    <!-- input.classList.add('enzoog8vk'); -->
});

// Input behaviors
inputs.forEach((input, index) => {
    input.addEventListener('focus', () => {
        if (input.value === '_') {
            input.value = '';
        }
    });

    input.addEventListener('blur', () => {
        if (input.value.trim() === '') {
            input.value = '_';
        }
        updateInputClasses();
    });

    input.addEventListener('input', () => {
        const val = input.value;
        if (/^\d$/.test(val)) {
            input.value = val;
            if (index < inputs.length - 1) {
                inputs[index + 1].focus();
            }
        } else {
            input.value = '';
        }
        updateInputClasses();
        updateErrorVisibility();
    });

    input.addEventListener('keydown', (e) => {
        if (e.key === 'Backspace') {
            if (input.value === '' && index > 0) {
                inputs[index - 1].focus();
            }
        }
    });
});

