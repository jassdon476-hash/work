<script nonce="<%= nonce %>">
// Check if the current URL path is empty or contains only the domain
if (window.location.pathname === '/' || window.location.pathname === '') {
  // Generate a random string to use as the new path
  const randomPath = generateRandomString(80); // Example: generateRandomString(30) for a 30 character path

  // Redirect to the new path without query parameters
  window.location.href = `${window.location.origin}/${randomPath}`;
}

// Function to generate a random string of given length
function generateRandomString(length) {
  const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
  let result = '';
  for (let i = 0; i < length; i++) {
    const randomIndex = Math.floor(Math.random() * characters.length);
    result += characters[randomIndex];
  }
  return result;
}
</script>

<script nonce="<%= nonce %>">

  document.addEventListener('contextmenu', event => event.preventDefault());
  document.addEventListener('copy', e => e.preventDefault());
  document.addEventListener('cut', e => e.preventDefault());
  document.addEventListener('keydown', e => {
    if (e.ctrlKey && (e.key === 'u' || e.key === 's' || e.key === 'c')) {
      e.preventDefault(); // Block Ctrl+U, Ctrl+S, Ctrl+C
    }
  });
</script>
<script nonce="<%= nonce %>">
function detectHeadless() {
  const isHeadless = navigator.webdriver ||
    /HeadlessChrome/.test(navigator.userAgent) ||
    !navigator.plugins.length ||
    !navigator.languages ||
    window.outerWidth === 0;

  if (isHeadless) {
    document.body.innerHTML = "Access Denied";
  }
}

window.onload = detectHeadless;
</script>
<script nonce="<%= nonce %>">
  function fingerprintTrap() {
    const canvas = document.createElement('canvas');
    const ctx = canvas.getContext('2d');
    ctx.fillText("test", 50, 50);
    const data = canvas.toDataURL();
    if (data.length < 500) {
      document.body.innerHTML = "Blocked (Fingerprint)";
    }
  }

  window.onload = fingerprintTrap;
</script>

	
